-->
<!DOCTYPE HTML>
<html>
<body>
    <div class="content">
        <div class="wrap">
            <div class="about">
                <h3>About</h3>
                @foreach (var x in Model)
                {
                    <img src="@x.ImgURL" alt="" />

                <p>@x.Description</p>
                }
            </div>
        </div>
    </div>
</body>
</html>


